<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.education.cs.binocularvision">
    <!-- "@string/app_name"-->
    <application
            android:allowBackup="true"
            android:icon="@drawable/icon"
            android:label="BinocularVision"
            android:roundIcon="@drawable/icon"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity
                android:name="com.education.amblyopiatreatment.activities.StatisticsActivity"
                android:exported="true"/>
        <activity
                android:name="com.education.amblyopiatreatment.activities.MainActivity"
                android:exported="true"
                android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name="com.education.amblyopiatreatment.activities.GameActivity"
                android:exported="true"
                android:screenOrientation="landscape"/>

        <meta-data
                android:name="preloaded_fonts"
                android:resource="@array/preloaded_fonts"/>
    </application>

</manifest>